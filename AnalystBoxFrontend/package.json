{
  "name": "@mobikitinc/analystbox",
  "version": "1.0.0",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mobikitinc/analyst.git",
    "directory": "AnalystBoxFrontend"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:clean": "rimraf dist",
    "build:transpile": "babel src --out-dir dist --copy-files",
    "build:pack": "npm pack",
    "build:all": "npm run build:clean && npm run build:transpile && npm run build:pack",
    "build:dev": "cross-env BABEL_ENV=development npm run build:clean && npm run build:transpile",
    "build:prod": "cross-env BABEL_ENV=production npm run build:all",
    "build": "npm run build:prod",
    "test": "nyc ava"
  },
  "dependencies": {
    "prop-types": "^15.5.7",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.9.2",
    "ava": "^3.2.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-minify": "^0.5.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nyc": "^15.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "ava": {
    "inherit": true,
    "files": [
      "tests/**/*.test.js"
    ],
    "babel": {
      "extensions": [
        "js",
        "css"
      ]
    }
  }
}
